{
  "$schema": "http://json.schemastore.org/template",
  "author": "Johannes Bildstein",
  "classifications": [ "Desktop" ],
  "name": "SpiderEye Application",
  "identity": "Bildstein.SpiderEye.App.CSharp",
  "groupIdentity": "Bildstein.SpiderEye.App",
  "shortName": "spidereye-app",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "MyApp",
  "preferNameDirectory": true,
  "symbols": {
    "no-sln": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Specifies if the solution file should be added"
    },
    "no-vscode": {
      "type": "parameter",
      "datatype": "bool",
      "defaultValue": "false",
      "description": "Specifies if the .vscode folder should not be added"
    },
    "project-file": {
      "type": "generated",
      "generator": "constant",
      "parameters": {
        "value": "csproj"
      },
      "replaces": "langproj"
    },
    "source-file": {
      "type": "generated",
      "generator": "constant",
      "parameters": {
        "value": "cs"
      },
      "replaces": "lang"
    },
    "package-version": {
      "type": "generated",
      "generator": "constant",
      "parameters": {
        "value": "1.0.0-beta.1"
      },
      "replaces": "0.0.0-marker.0"
    },
    "framework-core-version": {
      "type": "generated",
      "generator": "constant",
      "parameters": {
        "value": "netcoreapp3.1"
      },
      "replaces": "netframework-core-marker"
    },
    "framework-version": {
      "type": "generated",
      "generator": "constant",
      "parameters": {
        "value": "netcoreapp3.1"
      },
      "replaces": "netframework-marker"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "condition": "(no-sln)",
          "exclude": [ "MyApp.sln" ]
        },
        {
          "condition": "(no-vscode)",
          "exclude": [ ".vscode/**/*" ]
        }
      ]
    }
  ]
}
